{
    "locations": {
        "layouts": "layouts",
        "views": "views",
        "public": "public",
        "logs": "logs"
    },
    "log": {
        "path": "log",
        "autoinject": true,
        "type": "single"
    },
    "config": {
        "path": "config",
        "autoinject": true,
        "type": "single"
    },
    "lib": {
        "path": "lib",
        "autoinject": true,
        "type": "all"
    },
    "proxy": {
        "path": "proxy",
        "autoinject": true,
        "type": "all"
    },
    "errors": {
        "path": "errors",
        "autoinject": true,
        "aggregateOn": "errors",
        "type": "all"
    },
    "controllers": {
        "path": "controllers",
        "autoinject": true,
        "aggregateOn": "controllers",
        "type": "all"
    }
}