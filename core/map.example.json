{
    "locations": {
        "layouts": "layouts",
        "views": "views",
        "public": "public",
        "logs": "logs"
    },
    // Factrories:
    // single   - injected file or directory as one module;
    // all      - if (type == file) injected by single strategy others directory - injected recursively all files.
    "modules": {
        "log": {
            "path": "log",
            "type": "single"
        },
        "config": {
            "type": "single"
        },
        "lib": {
            "type": "all"
        }
    }
}