{
    "task": "grunt-contrib-concat",
    "name": "concat",
    "config": {
        "startup": {
            "files": [
                {
                    "src": ["<%= meta.js %>startup/**/*.js", "<%= meta.js %>app/**/*.js"],
                    "dest": "<%= meta.jsDest %>app.js"
                }
            ]
        },
        "css": {
            "files": [
                {
                    "src": "<%= meta.build %>css/**/*.css",
                    "dest": "<%= meta.cssDest %>app.css"
                }
            ]
        }
    }
}