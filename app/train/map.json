{
    "locations": {
        "layouts": "../public/views/shared/layouts",
        "views": "../public/views/",
        "public": "../public",
        "logs": "logs"
    },
    "modules": {
        "log": {
            "type": "single"
        },
        "config": {
            "type": "single"
        },
        "startup": {
            "type": "all"
        },
        "proxy": {
            "type": "all"
        },
        "errors": {
            "aggregateOn": "errors",
            "type": "all"
        },
        "controllers": {
            "aggregateOn": "controllers",
            "type": "all"
        }
    }
}